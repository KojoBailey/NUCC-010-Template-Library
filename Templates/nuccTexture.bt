#ifndef KOJO_NUCC_TEXTURE_BT
#define KOJO_NUCC_TEXTURE_BT

#include "nut.bt"

string nuccTextureREAD() {
    return Str("Dimensions: %u%s%u", width, "x", height);
}

typedef struct {
    BigEndian();
    uint16 unk00;
    uint16 width <name = "uint16 Width">;
    uint16 height <name = "uint16 Height">;
    uint16 unk06;
    uint32 size <name = "uint32 Size">;

    NUT t_NUT;
} nuccTexture;

#endif // KOJO_NUCC_TEXTURE_BT